<script type="text/javascript"> 
 $('.ppt li:gt(0)').hide();
 $('.ppt li:last').addClass('last'); 

var cur = $('.ppt li:first'); 

function animate() { cur.fadeOut( 1000 ); 
if ( cur.attr('class') == 'last' ) 
cur = $('.ppt li:first'); else cur = cur.next(); 
cur.fadeIn( 1000 ); } 
$(function() { setInterval( "animate()", 5000 ); } ); 

